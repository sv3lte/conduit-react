{
  "author": "Yoonge <7884008@qq.com>",
  "description": "Front-End of Conduit built with ESLint + Less + Prettier + React + TypeScript + Vite.",
  "keywords": [
    "Conduit",
    "ESLint",
    "Less",
    "Prettier",
    "React",
    "RealWorld",
    "TypeScript",
    "Vite"
  ],
  "name": "conduit-react",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "npx prettier --write src",
